{
  "name": "hyfn-org",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "node ./fetch-envs.js && node ./fetch-fr-envs.js && tsc --build ./packages/hyfn-client && tsc --build ./packages/hyfn-types && tsc --build ./packages/hyfn-server ",
    "typecheck": "tsc --noEmit",
    "deploy-prod": "sst deploy --stage production",
    "deploy-staging": "sst deploy --stage staging",
    "deploy-dev": "sst deploy --region eu-west-3 --stage development",
    "wrap": "node ../../wrapper.cjs",
    "sst-dev": "sst dev --region eu-west-3 --stage development",
    "start": "sst bind --region eu-west-3 --stage development  vite",
    "console": "sst console",
    "after-install": "node ./fetch-envs.js && node ./fetch-fr-envs.js && tsc --build ./packages/hyfn-client && tsc --build ./packages/hyfn-types && tsc --build ./packages/hyfn-server ",
    "fetch-envs": "node ./fetch-envs.js && node ./fetch-fr-envs.js",
    "build-ts": "tsc --build ./packages/hyfn-client && tsc --build ./packages/hyfn-types && tsc --build ./packages/hyfn-server ",
    "createDatabaseUser": "node ./createDatabaseUser.js"
  },
  "devDependencies": {
    "@serverless-stack/resources": "^1.18.4",
    "@tsconfig/node16": "^1.0.3",
    "aws-cdk-lib": "2.72.1",
    "colorette": "^2.0.19",
    "constructs": "10.1.156",
    "kysely-codegen": "^0.10.0",
    "prettier": "^2.8.8",
    "prettier-plugin-sql": "^0.14.0",
    "sst": "2.8.27",
    "typescript": "^4.9.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "kysely": "^0.24.2",
    "pg": "^8.10.0"
  }
}